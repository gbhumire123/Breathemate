{
  "expo": {
    "name": "BreatheMate",
    "slug": "breathemate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0f0f0f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f0f0f"
      }
    },
    "web": {},
    "plugins": [
      "expo-av",
      "expo-document-picker",
      "expo-font"
    ]
  }
}
